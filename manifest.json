{
   "manifest_version":2,
   "name":"Mush",
   "description":"Share links with your friends",
   "version":"2.1",
   "background": {
      "scripts": ["js/background.js"]
   },
   "browser_action":{
      "default_icon" : "/img/logo.png",
      "default_title": "Save to Mush",
      "default_popup": "popup.html"  
   },
   "permissions":[
      "gcm",
      "notifications",
      "storage",
      "activeTab",
      "webNavigation",
      "notifications"
   ]
}
